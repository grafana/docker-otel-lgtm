{
  ".github/workflows/ghcr-image-build-and-publish.yml": {
    "regex": ["cosign"]
  },
  ".github/workflows/release.yml": {
    "regex": ["cosign"]
  },
  "docker/Dockerfile": {
    "dockerfile": ["docker.io/redhat/ubi9", "docker.io/redhat/ubi9-micro"],
    "regex": [
      "cosign",
      "grafana",
      "loki",
      "opentelemetry-collector",
      "prometheus",
      "pyroscope",
      "tempo"
    ]
  },
  "mise.toml": {
    "mise": [
      "go",
      "go:github.com/grafana/oats",
      "java",
      "lychee",
      "node",
      "npm:json5",
      "npm:renovate",
      "rust"
    ],
    "regex": ["ghcr.io/super-linter/super-linter"]
  }
}
